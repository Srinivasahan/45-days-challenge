//Leetcode-https://leetcode.com/problems/best-time-to-buy-and-sell-stock-ii/submissions/993215583/
//c++
//13 Jul 23
class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n=prices.size(),sum=0;
        for(int i=1;i<n;i++){
            if(prices[i]>prices[i-1]) sum+=prices[i]-prices[i-1];
        }
        return sum;
    }
};
